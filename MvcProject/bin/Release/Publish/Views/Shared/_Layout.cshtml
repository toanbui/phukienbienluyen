@using BO;
@using Entities.Param;
@using Entities.Filter;
@using Entities;
@{
    var menuBo = new MenuBo();
    var param = new MenuParam();
    var MenuFilter = new MenuFilter() { Status = (int)Utilities.Constants.RecordStatus.Published };
    param.MenuFilter = MenuFilter;
    menuBo.Search(param);
    var listMenu = param.MenuEntitys;

    //Cấu hình
    var configBo = new ConfigurationBo();
    var paramConfig = new ConfigurationParam() { ConfigurationFilter = new ConfigurationFilter() };
    configBo.Search(paramConfig);

    var phone = paramConfig.ConfigurationEntitys.Where(i => i.Id == 2).FirstOrDefault().Value;
    var email = paramConfig.ConfigurationEntitys.Where(i => i.Id == 1).FirstOrDefault().Value;
    var address = paramConfig.ConfigurationEntitys.Where(i => i.Id == 3).FirstOrDefault().Value;
    var fanpage = paramConfig.ConfigurationEntitys.Where(i => i.Id == 4).FirstOrDefault().Value;
}
